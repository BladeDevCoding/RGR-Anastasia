{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/styles.css", "dest": "/api/styles.css" },
    { "src": "/api/images/(.*)", "dest": "/images/$1" },
    { "src": "/api/foto/(.*)", "dest": "/foto/$1" },
    { "src": "/api/foto_kafe/(.*)", "dest": "/foto_kafe/$1" },
    { "src": "/api/foto_deserts/(.*)", "dest": "/foto_deserts/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "buildCommand": "mkdir -p public",
  "outputDirectory": "public"
}